{
  "bindings": [
    {
      "name": "inputBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "input/{fileName}.{fileExtension}",
      "connection": "StorageConnection"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "InOut",
      "path": "output/{fileName}-Output.vtt",
      "connection": "StorageConnection"
    }
  ],
  "disabled": true
}